<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ErickGrandaP2.MainPage">
   
    
    <ScrollView>
        <VerticalStackLayout>
     
            <label x:Name="egranda_label2" text="{Binding Telefono}" />

            <Label x:Name="egranda_label3" Text="{Binding Nomnbre}" FontSize="16" HorizontalOptions="Start" />


            <Button x:Name="egranda_button1" Text="Recarga" Clicked="OnclickedRecarga" />
            
    </VerticalStackLayout>
    </ScrollView>         
    

</ContentPage>
